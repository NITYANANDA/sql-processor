            "userConfig": {
                "fn": "titlePersonView",
                "designer|params": [
                    "record"
                ],
                "implHandler": [
                    "var data = record.data,",
                    "    text = [], title;",
                    "",
                    "// Person name",
                    "text.push(data.firstName + \" \" + data.lastName);",
                    "",
                    "// Person identification",
                    "if (data.ssn) {",
                    "    text.push(data.ssn);",
                    "}",
                    "",
                    "title = text.join(\" | \");",
                    "return title;"
                ]
            },
            "designerId": "0f972b20-f17f-4019-8542-c06df16ba708"
